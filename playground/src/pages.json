{
  "globalStyle": {
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "Ano UI",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom"
  },
  "pages": [
    { "path": "pages/index/index" },
    {
      "path": "pages/basic/button/button",
      "style": { "navigationBarTitleText": "Button" }
    },
    {
      "path": "pages/basic/tag/tag",
      "style": { "navigationBarTitleText": "Tag" }
    },
    {
      "path": "pages/basic/cell/cell",
      "style": { "navigationBarTitleText": "Cell" }
    },
    {
      "path": "pages/form/input/input",
      "style": { "navigationBarTitleText": "Input" }
    },
    {
      "path": "pages/form/switch/switch",
      "style": { "navigationBarTitleText": "Switch" }
    },
    {
      "path": "pages/form/checkbox/checkbox",
      "style": { "navigationBarTitleText": "Checkbox" }
    },
    {
      "path": "pages/form/radio/radio",
      "style": { "navigationBarTitleText": "Radio" }
    },
    {
      "path": "pages/action/overlay/overlay",
      "style": { "navigationBarTitleText": "Overlay" }
    },
    {
      "path": "pages/action/notify/notify",
      "style": { "navigationBarTitleText": "Notify" }
    },
    {
      "path": "pages/action/notice-bar/notice-bar",
      "style": { "navigationBarTitleText": "Notice Bar" }
    }
  ],
  "easycom": {
    "autoscan": true,
    "custom": {
      "^A(.*)": "ano-ui/components/A$1/A$1.vue",
      "^U(.*)": "~/components/UnoUI/U$1/U$1.vue"
    }
  }
}
