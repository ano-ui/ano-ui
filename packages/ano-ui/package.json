{
  "name": "ano-ui",
  "type": "module",
  "version": "0.4.2",
  "packageManager": "pnpm@7.2.1",
  "description": "",
  "author": "Neil Lee <zguolee@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ano-ui/ano-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ano-ui/ano-ui.git"
  },
  "bugs": "https://github.com/ano-ui/ano-ui/issues",
  "keywords": [
    "unocss",
    "uni-app",
    "atomic-css",
    "atomic-css-engine",
    "css",
    "tailwind",
    "windicss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/ano-ui.umd.cjs",
      "import": "./dist/ano-ui.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/ano-ui.umd.mjs",
  "module": "./dist/ano-ui.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "components"
  ],
  "scripts": {
    "build": "esno scripts/components.ts && vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && vite build",
    "stub": "esno scripts/components.ts && vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && vite build --watch"
  },
  "peerDependencies": {
    "@ano-ui/preset": "^0.0.10",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@ano-ui/components": "workspace:*",
    "@ano-ui/preset": "workspace:*"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "unocss": "^0.48.0",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.3",
    "vue": "^3.2.45"
  }
}
