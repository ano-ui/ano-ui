{
  "name": "@ano-ui/monorepo",
  "version": "0.7.8",
  "private": true,
  "scripts": {
    "dev": "pnpm stub",
    "build": "rimraf packages/*/dist && pnpm -r --filter=./packages/* run build && pnpm -r run build-post",
    "play:mp-weixin": "npm -C packages/playground run dev:mp-weixin",
    "build:mp-weixin": "pnpm build && npm -C packages/playground run build:mp-weixin",
    "play:h5": "npm -C packages/playground run dev:h5",
    "build:h5": "pnpm build && npm -C packages/playground run build:h5",
    "play:app": "npm -C packages/playground run dev:app",
    "build:app": "pnpm build && npm -C packages/playground run build:app",
    "release": "bumpp package.json packages/*/package.json",
    "stub": "pnpm -r --filter=./packages/* --parallel run stub",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "new": "plop component"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@dcloudio/types": "^3.3.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "18.16.4",
    "@uni-helper/vite-plugin-uni-components": "^0.0.6",
    "@unocss/eslint-config": "^0.51.8",
    "bumpp": "^9.1.0",
    "eslint": "^8.39.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "plop": "^3.1.2",
    "rimraf": "^5.0.0",
    "scule": "^1.0.0",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "unocss": "^0.51.8",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
